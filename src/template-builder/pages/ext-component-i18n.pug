extends template

block pageVars
    - var pageTitle    = "i18n"
    - var pageSubTitle = "i18n"
    - var description  = "i18n"
    - var activeMenu   = "ext-component-i18n"

block breadcrumbs
    +breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Extensions"}, {name:"i18n"}])
append content
    include ../contents/ext-component-i18n.html

//- Vendor CSS
//------------------------------
//- Add vendor specific CSS
append vendorcss


//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs


//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
    script(src=app_assets_path+'/js/scripts/extensions/ext-component-i18n.js')
